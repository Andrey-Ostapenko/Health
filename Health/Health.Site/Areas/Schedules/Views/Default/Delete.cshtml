@using Health.Site.Areas.Schedules.Models
@model DefaultScheduleForm

@{
    ViewBag.Title = "Удаление расписания";
}

<h2>@Model.Message
    <p>
        @Html.RouteLink("Да", "Schedules_Default_Delete", new { schedule_id = Model.DefaultSchedule.Id, confirm = true })
